                         a.CONSUMO_VIRTUAL_LIQUIDACION, e.DETALLE_DESTADO
FROM            dbo.LIQUIDACION AS a LEFT OUTER JOIN
                         dbo.UNIDAD AS b ON a.CODIGO_UNIDAD_TRACTO = b.CODIGO_UNIDAD LEFT OUTER JOIN
                         dbo.UNIDAD AS c ON a.CODIGO_UNIDAD_SEMITRAILER = c.CODIGO_UNIDAD LEFT OUTER JOIN
                         dbo.GUIA_TRANSPORTISTA AS d ON d.CODIGO_GUIA = a.CODIGO_GUIA LEFT OUTER JOIN
                         dbo.ESTADO AS e ON e.CODIGO_ESTADO = a.CODIGO_ESTADO LEFT OUTER JOIN
                         dbo.TRABAJADOR AS f ON f.CODIGO_TRABAJADOR = a.CODIGO_TRABAJADOR
WHERE        (a.CODIGO_ESTADO = @estado)
ORDER BY a.CODIGO_LIQUIDACION</CommandText>
