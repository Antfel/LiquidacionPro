                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        a.CODIGO_LIQUIDACION, a.NUMERO_LIQUIDACION, f.APELLIDO_PATERNO_TRABAJADOR + ' ' + f.APELLIDO_MATERNO_TRABAJADOR + ', ' + f.NOMBRES_TRABAJADOR AS Expr1, d.DETALLE_GUIA, 
